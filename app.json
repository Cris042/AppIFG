{
  "expo": {
    "name": "app-if",
    "slug": "app-if",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },

    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "appcom.ios.Appif"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },

      "config":{
        "googleMaps": { "apiKey": "" }
      },

      "package": "appcom.android.Appif"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    }

  }
}
